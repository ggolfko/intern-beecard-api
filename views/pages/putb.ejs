          </div>
          <div class="col-md-6 mb-3">
            <input type="hidden" class="form-control" name="id" id="validationDefault04" placeholder="tel" value="<%= data.id %>">
          </div>
        </div>
        <div>
          <input type="submit" value="update" class="btn btn-primary">
        </div>

      </form>


    </div>
  </main>


  <footer>
    <% include ../partials/footer %>
  </footer>

</body>

</html>